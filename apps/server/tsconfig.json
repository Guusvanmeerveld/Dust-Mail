{
	"extends": "@dust-mail/tsconfig/nestjs.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"rootDir": "./src",
		"outDir": "./dist",
		"tsBuildInfoFile": ".tsbuildinfo",
		"paths": {
			"@src/*": ["./*"],
			"@utils/*": ["utils/*"],
			"@cache/*": ["cache/*"],
			"@auth/*": ["auth/*"],
			"@mail/*": ["mail/*"]
		}
	}
}
